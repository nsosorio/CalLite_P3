goal split_d7b_prj {d7b_prj = d7b_pwr + d7b_pag + d7b_prf }
!goal subcontinuity216   {d7b_prf - c216b = 0}

define D7C {std kind 'FLOW-DELIVERY' units 'CFS'} !Western Canal and Joint WD rice straw decomp diversions

