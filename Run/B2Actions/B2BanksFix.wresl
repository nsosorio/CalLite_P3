    condition month==JUN .and. int(B2Action5)==1 .and. int(B2Action5On)==1
    rhs   D_Banks_swp_td[Base]
    lhs<rhs penalty 0 }
  case fixBanks_B2Action6On {
    condition month==APR .and. int(B2Action6)==1 .and. int(B2Action6On)==1
